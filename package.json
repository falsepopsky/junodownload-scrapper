{
  "name": "junodownload-scrapper",
  "version": "0.2.0",
  "description": "junodownload scrapper",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
    "prettier-format": "prettier --cache --write .",
    "lint": "eslint",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "junodownload",
    "scrapper",
    "cheerio"
  ],
  "author": "Rodrigo Toledo",
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "*"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/junodownload-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/junodownload-scrapper/issues"
  }
}
