{
  "name": "junodownload-scrapper",
  "version": "0.2.0",
  "description": "junodownload scrapper for new and coming soon releases",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx dev/index.ts",
    "format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "tsc",
    "prepare": "husky install",
    "pre-commit": "pnpm format && pnpm lint",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "junodownload",
    "scrapper"
  ],
  "author": "Rodrigo Toledo",
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@swc/core": "^1.3.41",
    "@swc/jest": "^0.2.24",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "tsx": "^3.12.5",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/junodownload-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/junodownload-scrapper/issues"
  }
}
