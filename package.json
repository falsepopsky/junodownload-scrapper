{
  "name": "junodownload-scrapper",
  "version": "0.2.0",
  "description": "junodownload scrapper",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prettier-format": "prettier --cache --write .",
    "lint": "eslint . --ext .ts",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "junodownload",
    "scrapper",
    "cheerio"
  ],
  "author": "Rodrigo Toledo",
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "tsx": "^3.8.2",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/junodownload-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/junodownload-scrapper/issues"
  }
