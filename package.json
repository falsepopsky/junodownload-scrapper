{
  "name": "junodownload-scrapper",
  "description": "junodownload scrapper for new and coming soon releases",
  "version": "0.2.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "junodownload",
    "scrapper"
  ],
  "author": "falsepopsky",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/playground.ts",
    "format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "clean:workspace": "rm -rf node_modules dist",
    "clean:build": "rm -rf dist",
    "prebuild": "pnpm run clean:build",
    "build": "tsc --project tsconfig.build.json",
    "prepare": "husky install",
    "pre-commit": "pnpm format && pnpm lint",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@swc/core": "^1.3.53",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "undici": "^5.22.0"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/junodownload-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/junodownload-scrapper/issues"
  }
}
