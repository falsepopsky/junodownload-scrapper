{
  "name": "junodownload-scrapper",
  "version": "0.2.0",
  "description": "junodownload scrapper for new and coming soon releases",
  "exports": "./dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx dev/index.ts",
    "prettier-format": "prettier --cache -w .",
    "lint": "eslint . --cache --cache-location \"./node_modules/.cache/eslint/\"",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "prepare": "husky install",
    "pre-commit": "pnpm prettier-format && pnpm lint"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "node",
    "junodownload",
    "scrapper",
    "cheerio"
  ],
  "author": "Rodrigo Toledo",
  "license": "MIT",
  "dependencies": {
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.2",
    "tsx": "^3.12.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falsepopsky/junodownload-scrapper.git"
  },
  "bugs": {
    "url": "https://github.com/falsepopsky/junodownload-scrapper/issues"
  }
}
